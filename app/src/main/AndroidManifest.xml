<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.albert.myapplication">
    <!--開啟連外部網頁權限-->
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SecActivity" />
        <activity android:name=".webviewActivity" />
        <activity android:name=".ResultActivity" />
        <activity android:name=".ResultSecActivity" />
        <activity android:name=".FormWidetsActivity" />
        <activity android:name=".ThreadProgressbar" />
        <activity android:name=".FormExpertActivity" />
        <activity android:name=".ImagesMediaActivity"></activity>
    </application>

</manifest>